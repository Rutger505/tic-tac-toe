{
  "type": "module",
  "name": "tic-tac-toe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others --names \"next,websocket\" --prefix-colors \"cyan,green\"  \"npm run next-dev\" \"npm run ws-dev\"",
    "next-dev": "next dev",
    "ws-dev": "bun --hot src/app/api/websockets.ts",
    "production": "concurrently --kill-others --names \"next,websocket\" --prefix-colors \"cyan,green\"  \"npm run next-prod\" \"npm run ws-prod\"",
    "next-prod": "npm run start",
    "build": "next build",
    "start": "next start",
    "ws-prod": "bun src/app/api/websockets.ts",
    "lint": "next lint",
    "typeorm": "dotenv -e .env -- typeorm-ts-node-esm -d ./src/database/index.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.2.0",
    "@auth/typeorm-adapter": "^2.5.0",
    "@prisma/client": "^5.15.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@types/bun": "^1.1.5",
    "@types/express": "^4.17.21",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "express": "^4.19.2",
    "lucide-react": "^0.379.0",
    "next": "^14.2.11",
    "next-auth": "^5.0.0-beta.18",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/node": "^20.16.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "prisma": "^5.15.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
