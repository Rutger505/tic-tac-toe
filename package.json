{
  "type": "module",
  "name": "tic-tac-toe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others --names \"next,websocket\" --prefix-colors \"cyan,green\"  \"npm run next-dev\" \"npm run ws-dev\"",
    "next-dev": "next dev",
    "ws-dev": "bun --watch src/app/api/websocket.ts",
    "production": "concurrently --kill-others --names \"next,websocket\" --prefix-colors \"cyan,green\"  \"npm run next-prod\" \"npm run ws-prod\"",
    "next-prod": "next build && next start",
    "ws-prod": "bun src/app/api/websocket.ts",
    "lint": "next lint"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.2.0",
    "@prisma/client": "^5.15.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@types/bun": "^1.1.5",
    "@types/express": "^4.17.21",
    "bun": "^1.1.17",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "express": "^4.19.2",
    "lucide-react": "^0.379.0",
    "next": "14.2.3",
    "next-auth": "^5.0.0-beta.19",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.14.8",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "prisma": "^5.15.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
