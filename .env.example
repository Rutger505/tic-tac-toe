# Postgres database variables
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=user
POSTGRES_PASSWORD=secret
POSTGRES_DB=tic-tac-toe

# Base URL for the application (e.g. http://tic-tac-toe.localtest.me/)
BASE_URL=

# Random 32 byte string generated with `npx auth secret --raw`
AUTH_SECRET=
AUTH_TRUST_HOST=true
AUTH_URL=${BASE_URL}

# Google OAuth2 https://console.cloud.google.com/apis/credentials
AUTH_GOOGLE_ID=
AUTH_GOOGLE_SECRET=
